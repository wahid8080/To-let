<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goolemap">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/easy_rent"
        android:label="@string/app_name"
        android:roundIcon="@drawable/easy_rent"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Authorization.Login"></activity>
        <activity android:name=".YourWishList" />
        <activity android:name=".YourRentOwner" />
        <activity android:name=".UploadData.UploadInfoForBooking" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name=".Empty" />
        <activity android:name=".UploadData.UploadOwnerInfo" />
        <activity android:name=".UploadData.UploadRenderInfo" />
        <activity android:name=".UploadData.UploadRoomOrFlat" />
        <activity android:name=".FaceRoomOrFlat" />
        <activity android:name=".YourRent" />
        <activity android:name=".Authorization.Registration" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>